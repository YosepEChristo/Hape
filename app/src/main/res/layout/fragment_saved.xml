                    android:layout_height="@dimen/_30sdp"
                    android:layout_marginStart="@dimen/_210sdp"
                    android:layout_marginTop="@dimen/_75sdp"
                    android:layout_marginBottom="@dimen/_10sdp"
                    app:cardBackgroundColor="@color/brown"
                    app:cardCornerRadius="@dimen/_8ssp"
                    app:cardElevation="@dimen/_4ssp">

                    <ImageView
                        android:id="@+id/fu_btn_one_img"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:src="@drawable/ic_add_white_24" />
                </androidx.cardview.widget.CardView>
            </FrameLayout>

            <TextView
                android:id="@+id/fu_one_space"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_5sdp"
                android:textSize="@dimen/_10sdp"
                app:layout_constraintTop_toBottomOf="@id/fu_one" />

